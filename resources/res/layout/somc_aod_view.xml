            <com.sonymobile.keyguard.aod.StickerView android:layout_gravity="center_horizontal" android:id="@+id/aod_sticker_view" android:paddingTop="20dp" android:visibility="visible" android:layout_width="240dp" android:layout_height="240dp"/>
        </LinearLayout>
        <com.sonymobile.keyguard.aod.FingerPrintFeedBackView android:orientation="vertical" android:id="@+id/aod_fingerprint_feedback_text" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </FrameLayout>
</com.sonymobile.keyguard.aod.AodView>
